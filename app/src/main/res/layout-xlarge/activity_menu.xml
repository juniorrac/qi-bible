    <TextView
        android:id="@+id/tQ"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#F4511E"
        android:text="@string/app_name"
        android:gravity="center"
        android:textSize="50sp"/>
    <View
